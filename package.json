    "prompt-sync": "^4.1.5",
    "tildify": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-conventional": "8.0.0",
    "commitizen": "3.1.1",
    "cz-conventional-changelog": "2.1.0",
    "husky": "2.4.1",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.0"
